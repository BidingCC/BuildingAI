{
    "name": "@buildingai/ai-sdk-new",
    "version": "0.0.0",
    "private": true,
    "license": "MIT",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./providers": {
            "require": "./dist/providers/index.js",
            "import": "./dist/providers/index.js",
            "types": "./dist/providers/index.d.ts"
        }
    },
    "scripts": {
        "clean": "rimraf dist .turbo dist/build.tsbuildinfo",
        "check-types": "tsc --noEmit",
        "build": "rimraf dist && tsc -p tsconfig.build.json",
        "dev": "tsc -p tsconfig.build.json --watch",
        "format": "biome format --write .",
        "format:check": "biome ci .",
        "lint": "eslint . --quiet",
        "lint:fix": "eslint . --fix",
        "watch": "tsc -p tsconfig.build.json --watch",
        "test": "jest",
        "test:dev": "jest --watch"
    },
    "dependencies": {
        "@ai-sdk/anthropic": "^3.0.2",
        "@ai-sdk/deepseek": "^2.0.3",
        "@ai-sdk/google": "^3.0.2",
        "@ai-sdk/openai": "^3.0.2",
        "@ai-sdk/openai-compatible": "^2.0.2",
        "@ai-sdk/mcp": "^1.0.0",
        "@ai-sdk/provider": "^3.0.1",
        "@ai-sdk/provider-utils": "^4.0.2",
        "ai-sdk-ollama": "^3.0.1",
        "vercel-minimax-ai-provider": "^0.0.1",
        "@buildingai/constants": "workspace:*",
        "@buildingai/utils": "workspace:*",
        "@modelcontextprotocol/sdk": "1.21.0",
        "@types/pdf-parse": "^1.1.5",
        "ai": "^6.0.6",
        "mammoth": "^1.11.0",
        "pdf-parse": "^1.1.4",
        "tiktoken": "^1.0.22",
        "xlsx": "^0.18.5",
        "zod": "^4.3.5"
    },
    "devDependencies": {
        "@buildingai/eslint-config": "workspace:*",
        "@buildingai/typescript-config": "workspace:*"
    }
}