{
  "version": 2,
  "name": "buildingai",
  "buildCommand": "cd packages/web/buildingai-ui && pnpm run generate",
  "outputDirectory": "packages/web/buildingai-ui/.output/public",
  "framework": "nuxtjs",
  "installCommand": "pnpm install --filter=!./extensions/** --filter=!./templates/**",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./packages/web ./packages/@buildingai",
  "builds": [
    {
      "src": "packages/web/buildingai-ui/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".output/public"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/_nuxt/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NODE_VERSION": "22",
    "NUXT_BUILD_SSR": "false"
  },
  "regions": ["iad1"]
}
