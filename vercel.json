{
  "version": 2,
  "name": "buildingai",
  "buildCommand": "pnpm run build:web",
  "outputDirectory": "packages/web/buildingai-ui/.output/public",
  "framework": "nuxtjs",
  "installCommand": "pnpm install --filter=!./extensions/** --filter=!./templates/**",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/_nuxt/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NODE_VERSION": "22",
    "NUXT_BUILD_SSR": "false"
  },
  "regions": ["iad1"]
}
